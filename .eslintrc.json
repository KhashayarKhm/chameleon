{
		"env": {
			"browser": true,
				"es2021": true
		},
		"extends": [
			"airbnb-base",
			"plugin:json/recommended"
		],
		"parserOptions": {
			"ecmaVersion": 12
		},
		"rules": {
			"indent": [2, "tab", { "SwitchCase": 1 }],
			"no-tabs": 0,
			"object-shorthand": ["error", "properties"]
		}
}
